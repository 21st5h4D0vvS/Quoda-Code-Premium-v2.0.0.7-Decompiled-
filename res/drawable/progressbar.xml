<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item n1:id="@android:id/background"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <solid n1:color="#00000000" />
        </shape>
    </item>
    <item n2:id="@android:id/secondaryProgress"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <gradient n2:startColor="#77618f00" n2:endColor="#776da000" n2:angle="90.0" />
            </shape>
        </clip>
    </item>
    <item n3:id="@android:id/progress"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <gradient n3:startColor="#ff618f00" n3:endColor="#ff6da000" n3:angle="90.0" />
            </shape>
        </clip>
    </item>
</layer-list>